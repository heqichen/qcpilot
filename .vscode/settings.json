{
  "editor.tabSize": 2,
  "editor.insertSpaces": true,
  "editor.renderWhitespace": "trailing",
  "files.trimTrailingWhitespace": true,
  "search.exclude": {
    "**/.git": true,
    "**/.venv": true,
    "**/__pycache__": true,
    "common/**": true,
    "selfdrive/**": true,
    "system/**": true,
    "third_party/**": true,
    "tools/**": true,
  },
  "files.exclude": {
    "**/.git": true,
    "**/.venv": true,
    "**/__pycache__": true,
    "common/**": true,
    "selfdrive/**": true,
    "system/**": true,
    "third_party/**": true,
    "tools/**": true,
  },
  "python.analysis.exclude": [
    "**/.git",
    "**/.venv",
    "**/__pycache__",
    // exclude directories that should be using the symlinked version
    "common/**",
    "selfdrive/**",
    "system/**",
    "third_party/**",
    "tools/**",
  ],
  "files.associations": {
    "qpushbutton": "cpp",
    "optional": "cpp",
    "string": "cpp",
    "memory": "cpp",
    "qtimer": "cpp",
    "__config": "cpp",
    "chrono": "cpp",
    "core": "cpp",
    "algorithm": "cpp",
    "qcheckbox": "cpp",
    "mutex": "cpp",
    "shared_mutex": "cpp",
    "__mutex_base": "cpp"
  }
}
