Import('env', 'common', 'messaging')

evaluators_src = [
  "evaluators/can_valid_evaluator.cpp",
  "evaluators/car_recognized_evaluator.cpp",
  "evaluators/car_speed_evaluator.cpp",
  "evaluators/const_evaluator.cpp",
  "evaluators/resource_evaluator.cpp",
  "evaluators/calibrated_evaluator.cpp",
]

src = ["main.cpp", "cufud.cpp"] + evaluators_src
cufud_objs=env.Object(src)
env.Program("cufud", cufud_objs, LIBS=[common, messaging, 'zmq'])